<!doctype html>
<html>
  <head>
    <title><%=forumName%> - Users</title>
    <link rel="stylesheet" type="text/css" href="main.css" />
  </head>
  <body>
    <h1><%=forumName%></h1>
    <h2>Users</h2>
    <table>
    <tr><th>Username</th><th>First Name</th><th>Surname</th><th>Country</th></tr>
    <% users.forEach(function(user) { %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.firstname %></td>
        <td><%= user.surname %></td>
        <td><%= user.country %></td>
        <td><a href="/user/detail/<%= user.user_id  %> ">View Details</a></td>
    </tr>
    <% }) %>      
    </table>  
  </body>
</html>